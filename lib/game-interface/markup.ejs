<fieldset id="game-choose">
  <legend><%- __('game-interface.legend') %></legend>
  <select id="game-selection">
    <option data-args="[]" data-translatable="game-interface.empty-selection" value=""></option>
    <% games.forEach(function(game){ %>
      <option data-args="[]" data-translatable="<%=game.name%>.name" value="<%= game.name %>"></option>
    <% }) %>
  </select>
  <button id="game-starter"><%- __('game-interface.buttons.start') %></button>
  <button id="game-ender"><%- __('game-interface.buttons.leave') %></button>
</fieldset>
<% games.forEach(function(game){ %>
  <%- game.output() %>
<% }) %>
<%- bets.winningsMarkup() %>
